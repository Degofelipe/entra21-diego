6-   let numero = Number(prompt("Digite um número"))
      if (numero > 20 && numero < 90){
          console.log("O número está entre 20 e 90")
      } else {
          console.log("O número está fora da faixa")
      }
      console.log("Programa finalizado...")


8-     let numeroA = Number(prompt("Digite um número"))
    let numeroB = Number(prompt("Digite um número"))
    let numeroC = Number(prompt("Digite um número"))
     if (numeroA > numeroB && numeroA > numeroC){
         console.log(`O número ${numeroA} é o maior entre os três`)
     } else if (numeroB > numeroA && numeroB > numeroC){
         console.log(`O número ${numeroB} é o maior entre os três`)
     } else {
         console.log(`O número ${numeroC} é o maior entre os três`)
     }
     console.log("Programa finalizado...")



10-  let ladoA = Number(prompt("Digite um lado do triângulo"))
        let ladoB = Number(prompt("Digite um lado do triângulo"))
        let ladoC = Number(prompt("Digite um lado do triângulo"))
         if (ladoA == ladoB && ladoB == ladoC) {
         console.log("O triângulo é equilátero") 
          } else if (ladoA != ladoB && ladoA != ladoC && ladoB != ladoC) {
         console.log("O triângulo é escaleno")
         } else  {
         console.log("O triângulo é isósceles")
         }


11- let numeroA = Number(prompt("Digite um numero"))
        if (numeroA > 0) {
            let raiz = Math.sqrt(numeroA)
            console.log(`A raiz quadrada de ${numeroA} é de ${raiz.toFixed(2)}`)
        } else {
            console.log("Número inválido")
        }



12- let numero = Number(prompt("Digite um número"))
       if (numero % 5 == 0) {
           console.log("Número divisível por 5")
       } else {
           console.log("Número não divisível por 5")
       }



13 - let numero = Number(prompt("Digite um número"))
        if (numero > 0) {
            let raiz = Math.sqrt(numero)
            console.log(`A raiz quadrada de ${numero} é de ${raiz.toFixed(2)}`)
        } else {
            let quadrado = (numero ** 2)
            console.log(`O quadrado de ${numero} é de ${quadrado}`)
        }



14- let salario = Number(prompt("Digite o sálario"))
       let prestacao = Number(prompt("Digite o valor da prestação"))
        if (prestacao > salario / 100 * 20) {
            console.log("Empréstimo não concedido")
        } else {
            console.log("Empréstimo concedido")
        }



15-  let timeA = Number(prompt("Informe a quantidade de gols do time A"))
      let timeB = Number(prompt("Informe a quantidade de gols do time B"))
       if (timeA == timeB) {
           console.log("Empate")
       } else if (timeA > timeB) {
           console.log("Vitória do time A")
       } else {
           console.log("Vitória do time B")
       }



16-  let idade = Number(prompt("Digite sua idade"))
       if (idade < 16) {
           console.log("Não precisa votar")
       } else if (idade >= 18 && idade <= 65) {
           console.log("Voto obrigatório")
       } else {
           console.log("Voto opcional")
       }


17 -   let n1 = Number(prompt("digite um número"))
     let n2 = Number(prompt("digite um número"))
     let n3 = Number(prompt("digite um número"))
      if (n1 > n2 && n1 > n3) {
          if (n2 > n3)
          console.log(`${n1} , ${n2} , ${n3}`)
          } else if (n3 > n2) {
          console.log(`${n1} , ${n3} , ${n2}`) 
      } else if (n2 > n1 && n2 > n3 && n1> n3) {
          console.log(`${n2} , ${n1} , ${n3}`)
      } else {
          console.log(`${n3} , ${n2} , ${n3}`)
      }


18-  let idade = Number(prompt("Digite sua idade"))
    if (idade < 18 || idade > 67 ) {
       console.log("não pode doar sangue")
   } else {
       console.log("Pode doar sangue")
   }


19 - let dia = Number(prompt("Digite um número de 1 a 7"))
  switch(dia) {
      case 1 :
        console.log("Domingo")
        break
      case 2 :
        console.log("Segunda-feira")
        break
      case 3 :
        console.log("terça-feira")  
        break
      case 4 :
        console.log("Quarta-feira")  
        break
      case 5 : 
        console.log("Quinta-feira")
        break
      case 6 : 
        console.log("Sexta-feira")
        break
      case 7 : 
        console.log("Sábado")
        break
      default : 
        console.log("Número inválido") 
  }